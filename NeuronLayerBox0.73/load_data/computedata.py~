import numpy as np
No=1.0
D0=np.array([0.005,0.01,0.015,0.01,0.08,0.05,0.06,0.01,0.01,0.02,0.01,0.005])
L0=np.array([0.008,0.1,0.1,0.05,0.1,0.1,0.4,0.05,0.25,0.225,0.05,0.008])
R0=np.array([8,60,10,10,25,80,5,100,40,100,60,8])
D1=np.array([0.005,0,0.015,0.01,0.08,0,0.06,0,0,0,0,0.005])
L1=np.array([0.008,No,0.1,0.05,0.2,No,0.5,No,No,No,No,0.008])
R1=np.array([8,No,10,10,25,No,5,No,No,No,No,8])
g_L=np.array([0.01,0.01,0.1,0.1,0.03,0.9,0.03,0.01,0.01,0.05,0.01,0.01])

g0=(D0*D0)/(4*R0*L0)*1000000.
g1=(D1*D1)/(4*R1*L1)*1000000.
gl=g_L*L0*D0*10000.
print g0
print g1
print gl
